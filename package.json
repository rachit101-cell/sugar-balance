{
  "name": "sugar-streaker",
  "version": "1.0.0",
  "description": "Gamified family health platform focused on reducing sugar intake",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"nodemon backend/server.js\" \"cd frontend && vite\"",
    "dev:server": "nodemon backend/server.js",
